{
    "name": "dmitryrogolev/helper",
    "description": "Helper for the Laravel framework.",
    "version": "0.0.2",
    "type": "library",
    "keywords": [
        "helper",
        "laravel",
        "laravel-helper"
    ],
    "homepage": "https://github.com/dmitry-rogolev/Helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Rogolev",
            "email": "work.drogolev@internet.ru",
            "homepage": "https://dmitryrogolev.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^10.30"
    },
    "require-dev": {
        "orchestra/testbench": "^8.15",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "dmitryrogolev\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "dmitryrogolev\\Helper\\Tests\\": "tests/", 
            "dmitryrogolev\\Helper\\Tests\\Database\\Factories\\": "tests/database/factories/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit", 
        "pint": "vendor/bin/pint"
    }
}
